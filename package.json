{
  "name": "basic-environment-javascript",
  "version": "0.0.1",
  "description": "Basic Environment JavaScript (Babel, Eslint, Jest and Watch)",
  "main": "dist/common.js",
  "author": "Eduardo Paix√£o (@duduindo)",
  "license": "MIT",
  "scripts": {
    "server": "gulp watch",
    "build": "browserify ./src/js/app.js -t babelify --outfile ./dist/common.js",
    "test": "jest --config jest.config.json",
    "test:watch": "jest --config jest.config.json --watch",
    "test:coverage": "jest --config jest.config.json --coverage",
    "lint:eslint": "eslint src/js gulpfile.js --cache --ignore-path .gitignore; exit 0;",
    "gulp:js": "gulp js"
  },
  "keywords": [
    "environment",
    "javascript",
    "basic"
  ],
  "devDependencies": {
    "babel-cli": "*",
    "babel-eslint": "*",
    "babel-jest": "*",
    "babel-preset-latest": "*",
    "babelify": "*",
    "browser-sync": "^2.24.4",
    "browserify": "^16.2.2",
    "eslint": "*",
    "gulp": "^3.9.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.0",
    "gulplog": "^1.0.0",
    "html-loader-jest": "*",
    "http-server": "*",
    "jest": "*",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  }
}
